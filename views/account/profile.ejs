<% include ../partials/head %>
<% include ../partials/header %>
<!-- Register page start -->
<div class="sub-banner overview-bgi">
  <div class="container">
      <div class="breadcrumb-area">
          <h1>My Profile</h1>
          <ul class="breadcrumbs">
              <li><a href="/">Home</a></li>
              <li class="active">My Profile</li>
          </ul>
      </div>
  </div>
</div>
<div class="user-page content-area-14">
  <div class="container">
      <div class="row">
          <div class="col-lg-4 col-md-5 col-sm-12">
              <div class="user-profile-box mrb">
                      <!--header -->
                      <div class="header clearfix">
                          <h2>Versabit Technologies</h2>
                          
                          <img src="assets/img/logos/logo.png" alt="avatar" class="img-fluid profile-img">
                      </div>
                      <!-- Detail -->
                      <div class="detail clearfix">
                          <ul>
                              <li>
                                  <a href="/account" class="active">
                                      <i class="flaticon-user"></i>Profile
                                  </a>
                              </li>
                              <li>
                                  <a href="/invoice">
                                      <i class="flaticon-house"></i>My invoices
                                  </a>
                              </li>
                              <li>
                                  <a href="/invoice">
                                      <i class="flaticon-heart-shape-outline"></i>Active invoices
                                  </a>
                              </li>
                              <li>
                                  <a href="/invioce">
                                      <i class="flaticon-add"></i>Submit New Invoice
                                  </a>
                              </li>
                              <li>
                                  <a href="/">
                                      <i class="flaticon-gear"></i>Settings
                                  </a>
                              </li>
                              <li>
                                  <a href="/logout">
                                      <i class="flaticon-logout"></i>Log Out
                                  </a>
                              </li>
                          </ul>
                      </div>
                  </div>
          </div>
          <div class="col-lg-8 col-md-7 col-sm-12">
              <div class="my-address contact-2 widget">
                  <h3 class="heading">Profile Details</h3>
                  <form action="#" method="GET" enctype="multipart/form-data">
                      <div class="form-group">
                          <div class="form-check checkbox-theme">
                              <input class="form-check-input" type="radio" value="" id="">
                              <label class="form-check-label" for="Seller">
                                  Seller
                              </label>
                              </div>
                              <div class="form-check checkbox-theme">
                                  <input class="form-check-input" type="radio" value="" id="">
                                  <label class="form-check-label" for="Seller">
                                      Buyer
                                  </label>
                              </div>
                                  <div class="form-check checkbox-theme">
                                      <input class="form-check-input" type="radio" value="" id="">
                                      <label class="form-check-label" for="Seller">
                                          Financier
                                      </label>
                          </div>
                      </div>

                      <div class="row">
                          <div class="col-lg-12 ">
                              <div class="form-group name">
                                  <label>Company</label>
                                  <input type="text" name="name" class="form-control" placeholder="John Deo">
                              </div>
                          </div>
                          <div class="col-lg-12">
                              <div class="form-group email">
                                  <label>Contact Name</label>
                                  <input type="text" name="title" class="form-control" placeholder="Contact Name">
                              </div>
                          </div>
                          <div class="col-lg-12">
                              <div class="form-group email">
                                  <label>Designation</label>
                                  <input type="text" name="title" class="form-control" placeholder="Designation">
                              </div>
                          </div>
                          <div class="col-lg-12 ">
                              <div class="form-group subject">
                                  <label>Phone</label>
                                  <input type="text" name="phone" class="form-control" placeholder="Phone">
                              </div>
                          </div>
                          <div class="col-lg-12 ">
                              <div class="form-group ">
                                  <label>Email</label>
                                  <input type="email" name="email" class="form-control" placeholder="Email">
                              </div>
                          </div>
                          
                          <div class="col-lg-12">
                              <div class="send-btn">
                                  <button type="submit" class="btn btn-color btn-md btn-message">Save Changes</button>
                              </div>
                          </div>
                      </div>
                  
             
          </div></form>
      </div>
  </div>
</div>

<% include ../partials/footer %>